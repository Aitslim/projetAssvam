{% extends 'admin/admin_base.html.twig' %}

{% block title %}Ajouter une categoerie
{% endblock %}

{% block body %}

	<main>
		<div class="container-fluid px-4">
			<h1>Ajouter une categorie</h1>

			{{ form_start(form) }}
			{{ form_row(form.name) }}

			<button type="submit" class="btn btn-lg btn-primary my-3" action="{{ path('admin_category_add') }}">Valider</button>
			{{ form_end(form) }}
		</div>
	</main>
{% endblock %}
