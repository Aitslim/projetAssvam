{% extends 'base.html.twig' %}

{% block title %}Modifier une categoerie
{% endblock %}

{% block body %}

	<div class="container-fluid" style="margin-top:30px">

		<h5>Modifier une categorie</h5>

		{{ form_start(form) }}
		{{ form_row(form.name) }}

		<button type="submit" class="btn btn-primary btn-sm">Valider</button>
		<a href="{{ path('admin_category_list') }}" class="btn btn-primary btn-sm">Annuler</a>

		{{ form_end(form) }}
	</div>

{% endblock %}
